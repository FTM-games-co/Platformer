; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Platformer"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.1", "Mobile")
config/icon="res://icon.svg"

[autoload]

BackGroundMusic="*res://Audio/back_ground_music.tscn"
CoinEffect="*res://Audio/coin_effect.tscn"
JumpEffect="*res://Audio/jump_effect.tscn"
ClickEffect="*res://Audio/click_effect.tscn"
Game="*res://Util/Global/Game.gd"
Saveload="*res://Util/Global/Saveload.gd"
DeathEffect="*res://Audio/death_effect.tscn"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="mobile"
